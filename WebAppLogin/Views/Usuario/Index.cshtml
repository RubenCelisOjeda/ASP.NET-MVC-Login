@{
    ViewData["Title"] = "Index Page";
}

<div class="text-center">
    <h1 class="display-4">Login</h1>
</div>

<div class="container-fluid">
    <div class="row align-content-center">

        <div class="col-md-6">
             <div class="form-group">
                <label>Usuario</label>
                <input type="text" id="txtUsuario" class="form-control" width="200"/>
            </div>

            <div class="form-group">
                <label>Contraseña</label>
                <input type="password" id="txtPassword" class="form-control" width="200" />
            </div>

            <button id="btnLogin" class="btn btn-success">Login</button>
        </div>
       
    </div>
</div>

<script src="~/lib/ns.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>


<script type="text/javascript" src="~/js/usuario/index.js"></script>